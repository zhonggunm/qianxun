# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{js,java,vue}]
charset = utf-8

# 4 space indentation
[*.java]
indent_style = space
indent_size = 4

# 2 space indentation
[*.{js,vue}]
indent_style = space
indent_size = 2
